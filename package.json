{
  "name": "apis-search",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "fragments.css": "^1.0.38-alpha",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-instantsearch": "^5.1.0",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "npm run tailwind:css && react-scripts start",
    "build": "npm run tailwind:css && react-scripts build && npm run netlify:redirects",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "tailwind:css": "tailwind build src/tailwind.src.css -c  tailwind.js -o src/tailwind.css",
    "netlify:redirects": "echo 'https://api-search.netlify.com/* https://api-search.io/:splat 301!' > build/_redirects"
  },
  "devDependencies": {
    "tailwindcss": "^0.5.3"
  }
}
